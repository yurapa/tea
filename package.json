{
  "name": "tea",
  "version": "25.02.25",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yurapa/tea.git"
  },
  "engines": {
    "node": "^20.8.0 || ^22.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "email": "cp .env ./node_modules/react-email && email dev --dir email --port 3001",
    "postinstall": "prisma generate",
    "code:lint": "next lint",
    "code:lint-fix": "next lint --fix",
    "code:type-check": "tsc -b",
    "code:format": "prettier --check .",
    "code:format-fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:seed": "tsx ./db/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push"
  },
