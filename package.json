    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.14",
    "bufferutil": "^4.0.9",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.3.1",
